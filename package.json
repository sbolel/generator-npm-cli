{
  "name": "generator-npm-cli",
  "version": "0.2.0",
  "description": "Generator for a global npm module with a CLI.",
  "author": "Sinan Bolel <sinanbolel@gmail.com> (http://sinanbolel.com)",
  "preferGlobal": true,
  "keywords": [
    "cli",
    "command-line",
    "dev-tools",
    "generator",
    "yeoman-generator"
  ],
  "license": "MIT",
  "homepage": "https://github.com/sbolel/generator-npm-cli-module",
  "repository": {
    "type": "git",
    "url": "git@github.com:sbolel/generator-npm-cli-module.git"
  },
  "main": "lib/app.js",
  "bin": {
    "generator-npm-cli": "./bin/generator-npm-cli"
  },
  "directories": {
    "bin": "bin",
    "lib": "lib"
  },
  "files": [
    "./bin/**",
    "./lib/**",
    "./LICENSE",
    "./README.md",
    "./package.json"
  ],
  "engines": {
    "node": ">=5.7.0"
  },
  "scripts": {
    "build": "npm install -g ./",
    "start": "npm run debug",
    "debug": "DEBUG=* nodemon ./bin/generator-npm-cli",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "debug": "^2.2.0",
    "optimist": "^0.6.1",
    "prompt": "^1.0.0"
  },
  "devDependencies": {
    "nodemon": "^1.9.2"
  }
}
